                    </div><!-- /.col-md-4 -->
                </div><!--  /.callout-content -->
            </div><!--  /.container -->
        </div><!--  /.callout-action-area -->
    </section>


    Html.RenderPartial("~/Views/Home/Services.cshtml", (IEnumerable<Value>)ViewBag.Services);


    Html.RenderPartial("~/Views/Home/Pricing.cshtml", Model.Where(x => x.CategoryId != 2).Where(x => x.CategoryId != 6), new ViewDataDictionary { { "pricings", ViewBag.Pricings }, { "features", ViewBag.Features }, { "testimonials", ViewBag.Testimonials } });

}



